<div class="flexslider">
  <ul class="slides">
    {{range $slideImage := .Setting.SlideImages}}
      <li><img src="{{$slideImage.Image}}" /></li>
    {{end}}
  </ul>
</div>
<script>
    $(function(){
        $('.flexslider').flexslider({
            animation: 'slide',
            touch: true,
            directionNav: false
        });
    })
</script>
